{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "~Atoms": ["components/atoms/index.js"],
      "~Modules": ["components/modules/index.js"],
      "~Constants": ["constants/index.js"],
      "~Routes": ["constants/routes.js"],
      "~Store": ["store/*"],
      "~Api": ["api/index.js"],
